modules = ["python-3.12"]

[nix]
channel = "stable-23_05"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = [
  "sh",
  "-c",
  "python manage.py collectstatic --noinput && python manage.py runserver 0.0.0.0:3000",
]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3000
externalPort = 80
